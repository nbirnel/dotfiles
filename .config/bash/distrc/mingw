if [ -z "$USER" ]; then
    USER="$USERNAME"
fi
export USER

phone() {
    :~/.config/bash/distrc/mingw
    grep -i -- "$@" ~/teris-phone.csv
}

umask 0022

myip() {
    :~/.config/bash/distrc/mingw
    ipconfig | sed -n 's/^\s*IPv4 Address[. ]*: *//p'
}

_ps_pref="MINGW "

. $_BASH_DIST_CONFIGDIR/gnu

_OSRC="$_BASH_OS_CONFIGDIR/windows"
# vim:ft=sh

