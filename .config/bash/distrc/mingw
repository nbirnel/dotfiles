if [ -z "$USER" ]; then
    USER="$USERNAME"
fi
export USER

man2txt() {
    man $1 | col -b | u2d > $1.txt
}

npman() {
    man2txt $1 && notepad $1.txt
}

nppman() {
    man2txt $1 && notepad /p $1.txt
}

#FIXME does ming have this?
xo() {
    cat /dev/clipboard
}

phone() {
    grep -i -- "$@" ~/teris-phone.csv
}

umask 0022

myip() {
    ipconfig | sed -n 's/^\s*IPv4 Address[. ]*: *//p'
}

_ps_pref="MINGW "

. $_BASH_DIST_CONFIGDIR/gnu

_OSRC="$_BASH_OS_CONFIGDIR/windows"
# vim:ft=sh
