# apt aliases
alias paci='sudo apt-get install'
alias pacr='sudo apt-get remove'
alias pacar='sudo apt-get autoremove'
alias pacud='sudo apt-get update'
alias pacug='sudo apt-get upgrade'
alias pacl='dpkg -L'
alias paco='dpkg -S'
alias pacs='apt-cache search'

alias go='gnome-open'

_OSRC="$_BASH_OS_CONFIGDIR/linux"
